{
  "version": "1.0.0-*",
  "authors": [ "Malachi Burke" ],
  "description": "Caching wrappers for Fact.Extensions.Collection: Handles IMemoryCache and IDistributedCache",
  "packOptions": {
    "releaseNotes": ""
  },

  "buildOptions": {
    "xmlDoc": "true",
    /* Disable warnings about malformed XmlDoc stuff */
    "nowarn": [ "1591", "1571", "1572", "1573", "1574", "1587" ]
  },


  "dependencies": {
    "Fact.Extensions.Collection": "1.0.0-*",
    "Fact.Extensions.Serialization": "*",
    "Microsoft.Extensions.Caching.Abstractions": "1.0.0"
  },

  "frameworks": {
    /*
    "netstandard1.1": {
      "buildOptions": {
        "define": [ "NETSTANDARD1_1" ]
      }
    },*/
    "netstandard1.3": {
      "buildOptions": {
        "define": [ "NETSTANDARD1_3" ]
      }
    },
    "netstandard1.6": {
      "imports": "dnxcore50",
      "buildOptions": {
        "define": [ "NETSTANDARD1_6" ]
      },
      "dependencies": {
        "NETStandard.Library": "1.6.0"
      }
    }
  }
}
